{
  "manifest_version": 3,
  "browser_specific_settings": {
    "gecko": {
      "id": "cesarwamartinez@gmail.com"
    }
  },
  "name": "Tailwind Breack",
  "version": "1.0",
  "description": "Mira los breakpoints de tu pantalla de forma rapida. ",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "css": ["style.css"]
    }
  ],

  "web_accessible_resources": [
    {
      "resources": ["assets/*"],
      "matches": ["<all_urls>"]
    }],
  "permissions": []
}
